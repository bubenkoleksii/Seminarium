{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "IdentityUrl": "",
  "ReverseProxy": {
    "Routes": {
      "school": {
        "ClusterId": "school",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/school/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/school/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "school": {
        "Destinations": {
          "schoolApi": {
            "Address": "http://localhost:7001"
          }
        }
      }
    }
  }
}
